{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "school_handle"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The Username Schema",
            "default": "",
            "examples": [
                "mrgjune"
            ],
            "pattern": "^(.*)$"
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The Password Schema",
            "default": "",
            "examples": [
                "password"
            ],
            "pattern": "^(.*)$"
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The First_name Schema",
            "default": "",
            "examples": [
                "mara"
            ],
            "pattern": "^(.*)$"
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The Last_name Schema",
            "default": "",
            "examples": [
                "greene"
            ],
            "pattern": "^(.*)$"
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The Email Schema",
            "default": "",
            "examples": [
                "mara@gmail.com"
            ],
            "pattern": "^(.*)$"
        },
        "school_handle": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The school_handle Schema",
            "default": "",
            "examples": [
                "skid"
            ],
            "pattern": "^(.*)$"
        }
    }
}